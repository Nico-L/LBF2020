{
  "name": "atelierlbf2020",
  "version": "0.1.0",
  "description": "",
  "author": "Phil Hawksworth",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/philhawksworth/eleventail"
  },
  "scripts": {
    "build": "npm-run-all build:svelte build:css build:css2 build:html",
    "build:css": "postcss src/eleventy/_includes/css/tailwind.css -o src/eleventy/css/styles.css",
    "build:css2": "postcss src/svelte/reservationsMachines/addOn.css -o src/eleventy/css/resaMachinesAddOn.css",
    "build:html": "eleventy",
    "build:svelte": "rollup -c",
    "dev:svelte": "rollup -c -w",
    "watch": "npm-run-all --parallel dev:svelte watch:css watch:html",
    "watch:css": "postcss src/eleventy/_includes/css/tailwind.css -o src/eleventy/css/styles.css --watch",
    "watch:css2": "postcss src/svelte/reservationsMachines/addOn.css -o src/eleventy/css/resaMachinesAddOn.css --watch",
    "watch:html": "eleventy --serve --port=8181",
    "start": "npm run watch"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fullcalendar/core": "5.1.0",
    "@fullcalendar/daygrid": "5.1.0",
    "@fullcalendar/interaction": "5.1.0",
    "@fullcalendar/timegrid": "5.1.0",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@tailwindcss/custom-forms": "0.2.1",
    "autoprefixer": "^9.7.2",
    "axios": "^0.19.2",
    "cssnano": "^4.1.10",
    "html-minifier": "^4.0.0",
    "luxon": "1.24.1",
    "node-fetch": "2.6.0",
    "node-sass": "4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-import": "12.0.1",
    "rollup": "^2.21.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-postcss": "3.1.3",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^6.1.0",
    "svelte": "^3.20.1",
    "svelte-calendar": "^1.5.0",
    "svelte-fa": "^2.1.1",
    "svelte-fullcalendar": "0.4.0",
    "svelte-preprocess": "^4.0.8",
    "svelte-preprocess-postcss": "^1.1.1",
    "tailwindcss": "^1.4.6"
  },
  "devDependencies": {
    "rollup-plugin-css-only": "^2.0.0"
  },
  "keywords": []
}
