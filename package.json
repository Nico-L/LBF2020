{
  "name": "atelierlbf2020",
  "version": "0.1.0",
  "description": "",
  "author": "Phil Hawksworth",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/philhawksworth/eleventail"
  },
  "scripts": {
    "build": "npm-run-all build:svelte build:css build:html",
    "build:css": "postcss src/eleventy/_includes/css/tailwind.css -o src/eleventy/css/styles.css",
    "build:html": "eleventy",
    "build:svelte": "rollup -c",
    "dev:svelte": "rollup -c -w",
    "watch": "npm-run-all --parallel dev:svelte watch:css watch:html",
    "watch:css": "postcss src/eleventy/_includes/css/tailwind.css -o src/eleventy/css/styles.css --watch",
    "watch:html": "eleventy --serve --port=8181",
    "start": "npm run watch"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@fullcalendar/daygrid": "4.3.0",
    "@fullcalendar/interaction": "4.3.0",
    "@fullcalendar/timegrid": "4.3.0",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@tailwindcss/custom-forms": "0.2.1",
    "autoprefixer": "^9.7.2",
    "axios": "^0.19.2",
    "cssnano": "^4.1.10",
    "full-icu": "^1.3.1",
    "html-minifier": "^4.0.0",
    "luxon": "1.22.0",
    "node-fetch": "2.6.0",
    "node-sass": "4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "postcss-import": "12.0.1",
    "rollup": "^1.20.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-postcss": "2.0.5",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.0.0",
    "svelte-fullcalendar": "0.0.6",
    "svelte-preprocess": "^3.5.0",
    "svelte-preprocess-postcss": "^1.1.1",
    "tailwindcss": "^1.1.4"
  },
  "devDependencies": {},
  "keywords": []
}
